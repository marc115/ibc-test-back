generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model newuser {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  names     String
  lastNames String
  email     String  @unique
  password  String
  phone     String
  isActive  Boolean @default(true)
}
